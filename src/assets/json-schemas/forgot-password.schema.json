{
    "$id": "https://our-digital-bharat/login.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Login Schema",
    "type": "object",
    "properties": {
        "userName": {
            "type": "string",
            "title": "User Name",
            "description": "User Name of a user.",
            "maxLength": 30
        },
        "password": {
            "type": "string",
            "title": "Password",
            "description": "Password to be used for login into the system.",
            "maxLength": 30
        }
    },
    "additionalProperties": false,
    "required": [
        "userName",
        "password"
    ]
}